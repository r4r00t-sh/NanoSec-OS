{
    "name": "NanoSec Default",
    "version": "1.0",
    "colors": {
        "prompt_user_root": "#FF5555",
        "prompt_user_normal": "#50FA7B",
        "prompt_host": "#8BE9FD",
        "prompt_path": "#BD93F9",
        "prompt_symbol": "#F8F8F2",
        "status_ok": "#50FA7B",
        "status_warning": "#FFB86C",
        "status_error": "#FF5555",
        "status_info": "#8BE9FD",
        "output_header": "#FF79C6",
        "output_text": "#F8F8F2",
        "output_highlight": "#F1FA8C",
        "alert_critical": "#FF5555",
        "alert_warning": "#FFB86C",
        "alert_info": "#8BE9FD"
    },
    "prompt": {
        "format": "[{status}] {user}@{host}:{path}{symbol} ",
        "root_symbol": "#",
        "user_symbol": "$",
        "show_security_status": true
    },
    "banner": {
        "show_on_start": true,
        "ascii_art": true
    }
}